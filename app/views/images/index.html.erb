Index :
<h3><%= link_to 'Add Image', new_album_image_path(params[:album_id]) %><hr/>
<table>
<% @image.each do |image| %>
	<tr><td><%= image %> </td>
	<td><%= link_to image_tag(image.picture.url), image.picture.url %></td>
	<td><%= link_to 'Destroy', album_image_path(params[:album_id], image ), method: :delete, data: { confirm: 'Are you sure?'} %>
	<% end %></td></tr>
</table>
