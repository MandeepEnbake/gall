<div class="container">
<div class="row">
<div class="col-md-9 col-md-offset-3">
<h2>Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name),:class => 'form-horizontal') do |f| %>
  <div class="field">
  <h3><div class="col-md-2">
    <%= f.label :email, :class => 'label label-default' %>
    </div>
    <div class="col-md-8">
    <%= f.email_field :email, autofocus: true, :class => '.col-md' %>
   </div></h3> 
  </div>
<br/>
  <div class="field">

    <h3><div class="col-md-2"><%= f.label :password, :class => 'label label-default' %></div>
    <div class="col-md-2"><%= f.password_field :password, autocomplete: "off", :class => '.col-md' %></div></h3>
  </div>
<br/><br/>
  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in", :class => 'btn btn-sucess glyphicon glyphicon-hand-up' %>
  </div>
<% end %>
<div class="test">
<%= render "devise/shared/links" %>
</div>
</div>
</div>
</div>