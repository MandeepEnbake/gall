<h1>Albums List</h1>
<table>
	<tr>
		<th>Album Name</th>
	</tr>
	<% @album.each do |album| %>
	<tr>
		<td><%= album.title %></td>
		<td><%= link_to 'Show', album_path(album) %></td>
		<td><%= link_to 'Edit', edit_album_path(album) %></td>
		<td><%= link_to 'Destroy', album_path(album), method: :delete, data: { confirm: 'Are you sure?'} %></td>
	</tr>
	<% end %>
</table>